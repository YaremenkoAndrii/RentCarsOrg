        <fullName>End_date_cant_be_earlier_than_Start_Da</fullName>
        <active>true</active>
        <errorConditionFormula>Start_Date__c &gt;  End_Date__c   &amp;&amp;    ISPICKVAL(Status__c,  &apos;Open&apos; )</errorConditionFormula>
        <errorDisplayField>Start_Date__c</errorDisplayField>
        <errorMessage>End date can&apos;t be earlier than Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>New_deal_with_status_Open_only</fullName>
        <active>true</active>
        <errorConditionFormula>not  ISPICKVAL( Status__c , &apos;Open&apos;) &amp;&amp;  ISNEW()</errorConditionFormula>
